{
  "name": "prizo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "web:dev": "cd apps/web && npm run dev",
    "web:build": "cd apps/web && npm run build",
    "web:start": "cd apps/web && npm run start",
    "api:dev": "cd apps/api && .\\.venv\\Scripts\\python.exe -m uvicorn app.main:app --reload --port 8000",
    "dev": "concurrently -n web,api -c green,cyan \"npm run web:dev\" \"npm run api:dev\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
